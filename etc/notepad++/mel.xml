<NotepadPlus>
	<functionList>
		<!-- ====================================================== [ Mel ] -->

		<parser
			displayName="Mel"
			id         ="mel_syntax"
			commentExpr="(?s:/\*.*?\*/)|(?m-s://.*?$)"
		>
			<function
				mainExpr="proc\x20\K.+?(?=\()"
			>
				<functionName>
					<nameExpr expr=".*" />
				</functionName>
			</function>
		</parser>
	</functionList>
</NotepadPlus>